{
    "window.commandCenter": true,
    "terminal.integrated.defaultProfile.osx": "tmux-session",
    "terminal.integrated.profiles.osx": {
        "tmux-session": {
            "path": "tmux",
            "args": [
                "new-session",
                "-A",
                "-s",
                "vscode:${workspaceFolder}"
            ]
        }
    },
    "terminal.integrated.env.osx": {
        "LANG": "en_US.UTF-8"
    },
    "workbench.colorTheme": "Tomorrow Night Eighties",
    "workbench.colorCustomizations": {
        "statusBar.background": "#393939",
        "statusBar.foreground": "#cccccc",
        "statusBar.noFolderBackground": "#bd93f9",
        "statusBarItem.activeBackground": "#ffcc66",
        "statusBarItem.hoverBackground": "#50fa7b"
    },
    "editor.lineNumbers": "relative",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.snippetSuggestions": "none",
    "editor.autoSurround": "never",
    "editor.formatOnSave": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "[css]": {
        "editor.tabSize": 2
    },
    "files.trimTrailingWhitespace": true,
    "multiCommand.commands": [
        {
            "command": "extension.smartCtrlJ",
            "sequence": [
                {
                    "command": "workbench.action.focusBelowGroup"
                },
                {
                    "command": "workbench.action.terminal.focus",
                    "when": "!editorFocus"
                }
            ]
        },
        {
            "command": "extension.smartCtrlK",
            "sequence": [
                {
                    "command": "workbench.action.focusAboveGroup",
                    "when": "editorFocus"
                },
                {
                    "command": "workbench.action.focusFirstEditorGroup",
                    "when": "terminalFocus"
                }
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vim/vimrc",
    "github.copilot.advanced": {},
    "makefile.configureOnOpen": true,
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "files.associations": {
        "vimrc": "Vimscript"
    },
    "database-client.autoSync": true
}